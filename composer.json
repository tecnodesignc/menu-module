{
    "name": "tecnodesignc/menu-module",
    "type": "encore-module",
    "description": "Menu module for EncoreCMS 3. Handles all the menus.",
    "keywords": [
        "encorecms",
        "menu",
        "tree",
        "nested sets"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "info@tecnodesign.com.co",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/encorecms/issues",
        "source": "https://github.com/tecnodesignc/menu-modules"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/installers": "^2.1",
        "tecnodesignc/laravel-menus": "^9.0",
        "typicms/nestablecollection": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "3.8.*",
        "fzaninotto/faker": "1.4.0",
        "doctrine/dbal": "^3.3",
        "tecnodesignc/page-module": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Menu\\": ".",
            "Modules\\": "modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
